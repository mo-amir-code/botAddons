{
    "manifest_version": 3,
    "name": "ChatGPT Manager",
    "version": "1.0.0",
    "description": "ChatGPT Manager is a chrome extension that helps user to manage their AI conversations.",
    "permissions": [
        "storage"
    ],
    "icons": {
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "js": [
                "src/Content.tsx"
            ],
            "matches": [
                "https://chatgpt.com/*"
            ]
        }
    ]
}